@model IEnumerable<GarbageCollectorV2.Models.Budget>
 
@{
    ViewBag.Title = "Budget";
}

<h2>Budget</h2>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Month)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AmountOwned)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AmountOwned)
            </td>
            
        </tr>
    }

</table>